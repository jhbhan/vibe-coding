{
  "expo": {
    "name": "fixed-vibe-app",
    "slug": "fixed-vibe-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets//images/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "fixedvibe",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router", { "origin": "https://yourapp.expo.app" }]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}